{
  "name": "oh-my-commit-vscode",
  "version": "0.17.3",
  "description": "VSCode extension for AI-powered commit message generation with customizable providers",
  "engines": {
    "vscode": "^1.80.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oh-my-commit/oh-my-commit"
  },
  "categories": [
    "Other"
  ],
  "license": ["../../LICENSE"],
  "files": [
    "dist",
    "../../LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/main/extension.js",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "package": "vsce package --no-dependencies -o dist",
    "publish": "vsce publish --no-dependencies",
    "clean": "rimraf dist tsconfig.tsbuildinfo .turbo",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@oh-my-commit/shared": "workspace:^",
    "handlebars": "^4.7.8",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.2.1",
    "simple-git": "^3.27.0",
    "yaml": "^2.6.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^22.10.1",
    "@types/vscode": "^1.80.0",
    "@vscode/vsce": "^3.2.1",
    "esbuild": "^0.19.11",
    "nodemon": "^2.0.22",
    "tsup": "8.3.5",
    "typescript": "5.3.3",
    "vscode": "^1.1.37"
  },
  "activationEvents": [
    "onStartupFinished",
    "workspaceContains:.git"
  ],
  "contributes": {
    "views": {
      "ohMyCommit": [
        {
          "type": "webview",
          "id": "ohMyCommit.view",
          "name": "Oh My Commit"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ohMyCommit",
          "title": "Oh My Commit",
          "icon": "$(git-commit)"
        }
      ]
    },
    "commands": [
      {
        "command": "ohMyCommit.quickCommit",
        "title": "Oh My Commit: Quick Commit"
      },
      {
        "command": "ohMyCommit.selectModel",
        "title": "Oh My Commit: Select Model"
      },
      {
        "command": "ohMyCommit.openPreference",
        "title": "Oh My Commit: Open Preference"
      }
    ],
    "configuration": {
      "title": "Oh My Commit",
      "properties": {
        "ohMyCommit.ui.mode": {
          "type": "string",
          "enum": [
            "notification",
            "panel"
          ],
          "enumDescriptions": [
            "弹出通知，支持修改按钮",
            "在面板中显示完整提交界面"
          ],
          "default": "panel",
          "markdownDescription": "提交界面显示模式",
          "order": 1
        },
        "ohMyCommit.basic.uiLanguage": {
          "type": "string",
          "enum": [
            "system",
            "zh_CN",
            "en_US"
          ],
          "enumDescriptions": [
            "跟随系统语言",
            "中文",
            "English"
          ],
          "default": "system",
          "markdownDescription": "界面显示语言",
          "order": 2
        },
        "ohMyCommit.git.commitLanguage": {
          "type": "string",
          "enum": [
            "system",
            "zh_CN",
            "en_US"
          ],
          "enumDescriptions": [
            "跟随系统语言",
            "中文",
            "English"
          ],
          "default": "system",
          "markdownDescription": "Commit 信息语言",
          "order": 3
        },
        "ohMyCommit.model.id": {
          "type": "string",
          "enum": [
            "ohMyCommit.standard"
          ],
          "default": "ohMyCommit.standard",
          "markdownDescription": "选择自动提交（AC）服务的供应商与模型",
          "order": 4
        },
        "ohMyCommit.ai.apiKeys": {
          "type": "object",
          "markdownDescription": "AI 服务提供商的 API 密钥配置",
          "additionalProperties": {
            "type": "string"
          },
          "properties": {
            "openai": {
              "type": "string",
              "default": "",
              "markdownDescription": "OpenAI API 密钥",
              "order": 301
            },
            "anthropic": {
              "type": "string",
              "default": "",
              "markdownDescription": "Anthropic API 密钥",
              "order": 302
            }
          },
          "order": 30
        },
        "ohMyCommit.telemetry.shareLevel": {
          "type": "string",
          "enum": [
            "minimal",
            "basic",
            "full"
          ],
          "enumDescriptions": [
            "仅收集基本错误信息",
            "包含功能使用统计和性能数据",
            "额外包含 AI 生成结果的质量反馈"
          ],
          "default": "basic",
          "markdownDescription": "数据收集级别",
          "order": 51
        }
      }
    }
  }
}
