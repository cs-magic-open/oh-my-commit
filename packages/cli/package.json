{
  "name": "@oh-my-commit/cli",
  "version": "0.1.0",
  "bin": {
    "omc": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs --dts --platform node",
    "dev": "npm run build -- --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "eslint-config-custom": "*",
    "tsconfig": "*",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@oh-my-commit/extension": "workspace:*",
    "@oh-my-commit/shared": "workspace:*",
    "chalk": "^4.x",
    "commander": "^12.0.0",
    "inquirer": "^9.2.12",
    "neverthrow": "^8.1.1",
    "simple-git": "^3.22.0"
  }
}
