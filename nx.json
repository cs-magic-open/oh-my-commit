{
  "pluginsConfig": {
    "@nrwl/js": {
      "analyzeSourceFiles": true
    }
  },
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/packages/*/dist"
      ]
    },
    "dev": {
      "cache": true,
      "dependsOn": [
        "^dev"
      ],
      "outputs": [
        "{projectRoot}/packages/*/dist"
      ]
    },
    "clean": {
      "cache": true,
      "dependsOn": [
        "^clean"
      ],
      "outputs": [
        "{projectRoot}/packages/*/dist"
      ]
    },
    "type-check": {
      "cache": true,
      "dependsOn": [
        "^type-check"
      ]
    },
    "lint": {
      "cache": true,
      "dependsOn": [
        "^lint"
      ]
    },
    "lint:fix": {
      "cache": true,
      "dependsOn": [
        "^lint:fix"
      ]
    },
    "format": {
      "cache": true,
      "dependsOn": [
        "^format"
      ]
    },
    "build:extension": {
      "cache": true,
      "dependsOn": [
        "^build:extension"
      ],
      "outputs": [
        "{projectRoot}/packages/extension/dist"
      ]
    },
    "preview": {
      "cache": true,
      "dependsOn": [
        "^preview"
      ]
    }
  }
}